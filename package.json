  "scripts": {
    "start": "parcel ./src/localDevelopment/index.html",
    "build": "./scripts/build.sh general",
    "build:component": "./scripts/build.sh component",
    "new-component": "./scripts/new-component.sh",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|html)\"",
    "prettify": "yarn run format && yarn run lint:fix",
    "full-check": "yarn run prettify && yarn run typescript",
    "test": "./scripts/test.sh all",
    "test:component": "./scripts/test.sh single",
    "telicon:jsx": "node ./scripts/update-telicon.js > src/components/Telicon/src/List.tsx",
    "telicon:types": "svgr --template ./templates/telicon-svgr-types.js --no-prettier --typescript --config-file ./svgrrc.config.js node_modules/@sipster/telicon/telicon.svg > src/components/Telicon/src/telicon-types.ts",
    "telicon": "yarn telicon:jsx && yarn telicon:types",
    "prepare": "husky install",
    "release": "release-it --npm.skipChecks"
  },
  "publishConfig": {
    "access": "public"
  }
